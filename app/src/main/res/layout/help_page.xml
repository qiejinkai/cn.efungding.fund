<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="cn.efunding.fund.activity.HelpActivity">

    <include layout="@layout/commen_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/bar_height"
        android:layout_alignParentTop="true"
        android:id="@+id/include3"></include>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="this is help activity"
        android:layout_below="@+id/include3"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/include4"
        android:layout_alignParentEnd="true"
        android:id="@+id/textView" />
    <include layout="@layout/commen_footer"
        android:layout_width="match_parent"
        android:layout_height="@dimen/footer_height"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/footer_marginTop"
        android:layout_marginBottom="@dimen/footer_marginBottom"
        android:id="@+id/include4"></include>


</RelativeLayout>
